{
    "name": "graze/gigya-client",
    "description": "REST Client for Gigya API",
    "keywords": [
        "graze",
        "gigya-client"
    ],
    "homepage": "https://github.com/graze/gigya-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Harry Bragg",
            "email": "harry.bragg@graze.com",
            "role": "Developer"
        },
        {
            "name": "Graze Developers",
            "email": "developers@graze.com",
            "homepage": "http://www.graze.com",
            "role": "Development Team"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "^5.3",
        "illuminate/support": "^5.0",
        "graze/standards": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4",
        "mockery/mockery": "^0.9.4",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Graze\\Gigya\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Graze\\Gigya\\Test\\": "tests/src",
            "Graze\\Gigya\\Test\\Unit\\": "tests/unit",
            "Graze\\Gigya\\Test\\Functional\\": "tests/functional",
            "Graze\\Gigya\\Test\\Performance\\": "tests/performance"
        }
    }
}

{
    "name": "graze/gigya-client",
    "description": "REST Client for Gigya API",
    "keywords": [
        "graze",
        "gigya-client",
        "gigya"
    ],
    "homepage": "https://github.com/graze/gigya-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Harry Bragg",
            "email": "harry.bragg@graze.com",
            "role": "Developer"
        },
        {
            "name": "Graze Developers",
            "email": "developers@graze.com",
            "homepage": "http://www.graze.com",
            "role": "Development Team"
        }
    ],
    "require": {
        "php": "^5.5|^7.0",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/support": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.36 | ^5.2",
        "mockery/mockery": "^0.9.4",
        "squizlabs/php_codesniffer": "^3",
        "graze/standards": "^2.0",
        "sebastian/comparator": "^1.2.4"
    },
    "autoload": {
        "psr-4": {
            "Graze\\Gigya\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Graze\\Gigya\\Test\\": "tests/src",
            "Graze\\Gigya\\Test\\Unit\\": "tests/unit",
            "Graze\\Gigya\\Test\\Integration\\": "tests/integration",
            "Graze\\Gigya\\Test\\Performance\\": "tests/performance"
        }
    },
    "config": {
        "platform": {
            "php": "7.2"
        }
    }
}
